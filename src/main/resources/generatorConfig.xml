<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

    <context id="mms" targetRuntime="MyBatis3DynamicSql">

        <plugin type="org.mybatis.generator.plugins.CachePlugin">
        </plugin>
        <plugin type="org.mybatis.generator.plugins.FluentBuilderMethodsPlugin"/>
        <plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin"/>
        <plugin type="org.mybatis.generator.plugins.RowBoundsPlugin"/>
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin"/>
        <plugin type="org.mybatis.generator.plugins.ToStringPlugin"/>

        <commentGenerator>
            <property name="suppressDate" value="true"/>
        </commentGenerator>

        <connectionFactory>
            <property name="driverClass" value="com.mysql.jdbc.Driver"/>
            <property name="connectionURL" value="jdbc:mysql://10.1.87.109:3306/zhaolei"/>
            <property name="userId" value="root"/>
            <property name="password" value="123456"/>
        </connectionFactory>

        <javaModelGenerator targetPackage="com.zl.first.test.model" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <javaClientGenerator type="XMLMAPPER" targetPackage="com.zl.first.test.mapper" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <table tableName="zl_%">
            <domainObjectRenamingRule searchString="^zl" replaceString=""/>
        </table>
    </context>
</generatorConfiguration>